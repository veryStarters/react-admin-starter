section {
  line-height: 2;
  &.anchor {
    position: fixed;
    right: 40px;
    :global {
      .ant-anchor-wrapper {
        background-color: #ddd;
        border-radius: 10px;
        padding: 10px;
      }
    }
  }
  .title {
    font-weight: bold;
    font-size: 16px;
  }
  .subTitle {
    font-weight: bold;
    font-size: 14px;
    margin: 5px 0;
  }
  .part {
    margin: 10px 0;
    font-size: 14px;
  }
}
